
language: node_js
node_js:
  - "12"
before_script:
  - cd slides
script: yarn install && gulp
deploy:
  provider: pages
  skip-cleanup: true
  github-token: $GITHUB_TOKEN  # Set in travis-ci.org dashboard, marked secure
  email: $GITHUB_EMAIL
  name: $GITHUB_USERNAME
  verbose: true
  keep-history: true
  local-dir: slides/build/dist
  target_branch: gh-pages 
  on:
    branch: master 
